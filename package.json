{
  "name": "templ-project-javascript",
  "version": "0.0.1",
  "description": "Javascript Project Template",
  "main": "dist/index.js",
  "scripts": {
    "build": "rollup -c --environment NODE_ENV:production",
    "change:language": "node .scripts/change-language.js",
    "clean": "rimraf coverage dist",
    "inspector": "jsinspector server --port 9543",
    "jscpd:html": "npm run jscpd -- --reporters html",
    "lint:write": "npm run lint -- --fix",
    "lint:watch": "nodemon --exec 'npm run lint'",
    "prettier:check": "npm run prettier -- --list-different",
    "prettier:write": "npm run prettier -- --write",
    "prebuild": "npm run clean",
    "prerelease": "npm run build",
    "pretest": "npm run clean",
    "release": "release-it --ci --no-npm.publish",
    "test:junit": "npm run test -- --reporter mocha-junit-reporter --reporter-options mochaFile=.junit.xml",
    "test:watch": "nodemon --exec 'npm run test'",
    "git-hook:pre-commit": "npm run prettier:write && npm run lint:write && npm run jscpd && npm run test",
    "prettier": "prettier ./{src,test}/**/*.js",
    "jscpd": "jscpd ./src --blame --format javascript",
    "lint": "eslint ./{src,test}/**/*.{js,jsx}",
    "test": "npm run test:single -- './test/**/*.test.js'",
    "test:single": "nyc --reporter=html --reporter=text --extension .js mocha --forbid-only"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dragoscirjan/template-javascript.git"
  },
  "author": "Dragos Cirjan",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dragoscirjan/template-javascript/issues"
  },
  "homepage": "https://github.com/dragoscirjan/template-javascript#readme",
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.6",
    "@babel/node": "^7.8.7",
    "@babel/preset-env": "^7.9.6",
    "@babel/register": "^7.9.0",
    "@release-it/conventional-changelog": "^1.1.4",
    "@rollup/plugin-babel": "^5.0.2",
    "babel-eslint": "^10.1.0",
    "chai": "^4.2.0",
    "eslint": "^6.8.0",
    "eslint-plugin-mocha": "^6.3.0",
    "jscpd": "^2.0.16",
    "jscpd-badge-reporter": "^1.1.3",
    "jsinspector": "^2.0.4",
    "mocha": "^7.1.2",
    "mocha-junit-reporter": "^1.23.3",
    "nodemon": "^2.0.3",
    "nyc": "^15.0.1",
    "prettier": "^2.0.5",
    "release-it": "^13.5.8",
    "rimraf": "^3.0.2",
    "rollup": "^2.10.9",
    "yorkie": "^2.0.0"
  },
  "gitHooks": {
    "pre-commit": "npm run git-hook:pre-commit && git add ."
  },
  "dependencies": {}
}
